# Ignore node_modules in both frontend and backend
auth-app-frontend/node_modules/
auth-backend/node_modules/

# Ignore .env files in both frontend and backend
auth-app-frontend/.env
auth-backend/.env

# Ignore build and dist folders if generated
auth-app-frontend/build/
auth-app-frontend/dist/
auth-backend/build/
auth-backend/dist/

# Ignore logs
**/*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore system/OS files
.DS_Store
Thumbs.db

# Ignore IDE/editor config
.vscode/
.idea/

# Ignore coverage and temp files
**/coverage/
**/.next/
**/.turbo/
**/.cache/
*.zip
*.tar
*.gz

# Optional: ignore uploads or local dumps
**/uploads/
**/dump/
